<h1>Articles</h1>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article %>
    </li>
  <% end %>
</ul>

 <% if current_user %>
    <%= link_to 'logout', logout_path %>
 <% else %>
    <%= link_to 'Registration', sign_up_path %>
    <%= link_to 'Login', sign_in_path %>
<% end %>
